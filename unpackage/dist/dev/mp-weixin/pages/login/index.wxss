.content {
  padding: 20px;
}
.bg-set {
  position: fixed;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  z-index: -1;
}
.content-item {
  margin-top: 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.content-item .label {
  min-width: 48px;
  font-size: 26rpx;
  text-align: right;
}
.content-item .input {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.content-item .smscode {
  margin-left: 10px;
}
.protocol {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-top: 10px;
}
.protocol .text {
  font-size: 26rpx;
  margin-left: -10px;
}
.protocol .item {
  color: blue;
}
.login-btn {
  margin-top: 48px;
}

